<div class="app-page-title" style="background:#DBDADA !important;">
    <div class="row">
        <div class="col-md-9">
            <div class="page-title-wrapper">
                <div class="page-title-heading">
                    @if (Context.Session.GetString("BankName").ToUpper() == "CANARA")
                    {
                        <img style="margin-left:10px" src="~/Images/CANARASCVLogo.png" asp-controller="Dashboard" asp-action="Index" />
                    }
                    else if (Context.Session.GetString("BankName").ToUpper() == "BOI")
                    {

                        <img style="margin-left:10px" src="~/Images/BOISCVLogo.png" asp-controller="Dashboard" asp-action="Index"/>
                    }
                    else if (Context.Session.GetString("BankName").ToUpper() == "MACRO")
                    {
                        <img style="margin-left:-3px" src="~/Images/MG-Logo-transparant_120_40.png" asp-controller="Dashboard" asp-action="Index"/>
                    }
                    else if (Context.Session.GetString("BankName").ToUpper() == "ICICI")
                    {
                        <img src="data:image/png;base64,"
                             title="ICICI UK" asp-controller="Dashboard" asp-action="Index" alt="">
                    }
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="pull-right">
                <h3 style="padding-top:7%" id="pageTitle"></h3>
            </div>
        </div>
    </div>
</div>

